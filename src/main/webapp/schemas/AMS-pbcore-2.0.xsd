<?xml version="1.0" encoding="UTF-8" ?>
<schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
		targetNamespace="http://www.pbcore.org/PBCore/PBCoreNamespace.html"
		xmlns="http://www.pbcore.org/PBCore/PBCoreNamespace.html" elementFormDefault="qualified"
		version="2.0RC1">
	<import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
	<import namespace="http://purl.org/dc/elements/1.1/" schemaLocation="simpledc20021212.xsd"/>
	<element name="pbcoreDescriptionDocument" type="pbcoreDescriptionDocumentType">
	</element>
	<complexType name="pbcoreDescriptionDocumentType">
		<sequence>
			<element maxOccurs="unbounded" minOccurs="0" name="pbcoreAssetType" type="string">
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="pbcoreAssetDate" type="dateStringType">
			</element>
			<element maxOccurs="unbounded" minOccurs="1" name="pbcoreIdentifier" type="requiredSourceVersionStringType">
			</element>
			<element maxOccurs="unbounded" minOccurs="1" name="pbcoreTitle" type="titleStringType">
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="pbcoreSubject" type="subjectStringType">
			</element>
			<element maxOccurs="unbounded" minOccurs="1" name="pbcoreDescription" type="descriptionStringType">
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="pbcoreGenre" type="sourceVersionStringType">
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="pbcoreRelation">
				<complexType>
					<sequence>
						<element maxOccurs="1" minOccurs="1" name="pbcoreRelationType" type="sourceVersionStringType">
						</element>
						<element maxOccurs="1" minOccurs="1" name="pbcoreRelationIdentifier" type="string">
						</element>
					</sequence>
				</complexType>
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="pbcoreCoverage">
				<complexType>
					<sequence>
						<element maxOccurs="1" minOccurs="1" name="coverage" type="string">
						</element>
						<element maxOccurs="1" minOccurs="0" name="coverageType">
							<simpleType>
								<restriction base="string">
									<enumeration value="Spatial"/>
									<enumeration value="Temporal"/>
								</restriction>
							</simpleType>
						</element>
					</sequence>
				</complexType>
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="pbcoreAudienceLevel" type="sourceVersionStringType">
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="pbcoreAudienceRating" type="sourceVersionStringType">
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="pbcoreCreator">
				<complexType>
					<sequence>
						<element maxOccurs="1" minOccurs="1" name="creator" type="affiliatedStringType">
						</element>
						<element maxOccurs="unbounded" minOccurs="0" name="creatorRole" type="sourceVersionStringType">
						</element>
					</sequence>
				</complexType>
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="pbcoreContributor">
				<complexType>
					<sequence>
						<element maxOccurs="1" minOccurs="1" name="contributor" type="affiliatedStringType">
						</element>
						<element maxOccurs="unbounded" minOccurs="0" name="contributorRole" type="contributorStringType">
						</element>
					</sequence>
				</complexType>
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="pbcorePublisher">
				<complexType>
					<sequence>
						<element maxOccurs="1" minOccurs="1" name="publisher" type="affiliatedStringType">
						</element>
						<element maxOccurs="unbounded" minOccurs="0" name="publisherRole" type="sourceVersionStringType">
						</element>
					</sequence>
				</complexType>
			</element>
			<element name="pbcoreRightsSummary" type="rightsSummaryType" maxOccurs="unbounded" minOccurs="0">
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="pbcoreInstantiation" type="instantiationType">
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="pbcoreAnnotation" type="annotationStringType">
			</element>
		</sequence>
	</complexType>
	<complexType name="instantiationType">
		<sequence>
			<element maxOccurs="unbounded" minOccurs="1" name="instantiationIdentifier" type="requiredSourceVersionStringType">
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="instantiationDate" type="dateStringType">
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="instantiationDimensions" type="technicalStringType">
			</element>
			<element maxOccurs="1" minOccurs="0" name="instantiationPhysical" type="string">
			</element>
			<element maxOccurs="1" minOccurs="0" name="instantiationDigital" type="string">
			</element>
			<element minOccurs="0" name="instantiationStandard" type="string">
			</element>
			<element maxOccurs="1" minOccurs="1" name="instantiationLocation" type="string">
			</element>
			<element maxOccurs="1" minOccurs="0" name="instantiationMediaType" type="string">
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="instantiationGenerations" type="string">
			</element>
			<element maxOccurs="1" minOccurs="0" name="instantiationFileSize" type="technicalStringType">
			</element>
			<element maxOccurs="1" minOccurs="0" name="instantiationTimeStart" type="string">
			</element>
			<element maxOccurs="1" minOccurs="0" name="instantiationDuration" type="string">
			</element>
			<element maxOccurs="1" minOccurs="0" name="instantiationDataRate" type="technicalStringType">
			</element>
			<element maxOccurs="1" minOccurs="0" name="instantiationColors" type="string">
			</element>
			<element maxOccurs="1" minOccurs="0" name="instantiationTracks" type="string">
			</element>
			<element maxOccurs="1" minOccurs="0" name="instantiationChannelConfiguration" type="string">
			</element>
			<element name="instantiationLanguage" type="string" maxOccurs="1" minOccurs="0">
			</element>
			<element maxOccurs="1" minOccurs="0" name="instantiationAlternativeModes" type="string">
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="instantiationEssenceTrack" type="essenceTrackType">
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="instantiationRelation">
				<complexType>
					<sequence>
						<element maxOccurs="1" minOccurs="1" name="instantiationRelationType"  type="sourceVersionStringType">
						</element>
						<element maxOccurs="1" minOccurs="1" name="instantiationRelationIdentifier" type="string">
						</element>
					</sequence>
				</complexType>
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="instantiationAnnotation" type="annotationStringType">
			</element>
		</sequence>
	</complexType>
	<complexType name="essenceTrackType">
		<sequence>
			<element maxOccurs="1" minOccurs="0" name="essenceTrackType" type="string">
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="essenceTrackIdentifier" type="sourceVersionStringType">
			</element>
			<element maxOccurs="1" minOccurs="0" name="essenceTrackStandard" type="string">
			</element>
			<element maxOccurs="1" minOccurs="0" name="essenceTrackEncoding" type="sourceVersionStringType">
			</element>
			<element maxOccurs="1" minOccurs="0" name="essenceTrackDataRate" type="technicalStringType">
			</element>
			<element maxOccurs="1" minOccurs="0" name="essenceTrackFrameRate" type="string">
			</element>
			<element minOccurs="0" name="essenceTrackPlaybackSpeed" type="string">
			</element>
			<element maxOccurs="1" minOccurs="0" name="essenceTrackSamplingRate" type="string">
			</element>
			<element maxOccurs="1" minOccurs="0" name="essenceTrackBitDepth" type="string">
			</element>
			<element maxOccurs="1" minOccurs="0" name="essenceTrackFrameSize" type="string">
			</element>
			<element maxOccurs="1" minOccurs="0" name="essenceTrackAspectRatio" type="string">
			</element>
			<element maxOccurs="1" minOccurs="0" name="essenceTrackTimeStart" type="string">
			</element>
			<element maxOccurs="1" minOccurs="0" name="essenceTrackDuration" type="string">
			</element>
			<element maxOccurs="1" minOccurs="0" name="essenceTrackLanguage" type="string">
			</element>
			<element maxOccurs="unbounded" minOccurs="0" name="essenceTrackAnnotation" type="annotationStringType">
			</element>
		</sequence>
	</complexType>
	<complexType name="dateStringType">
		<simpleContent>
			<extension base="string">
				<attribute name="dateType"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="sourceVersionStringType">
		<simpleContent>
			<extension base="string">
				<attributeGroup ref="sourceVersionGroup"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="requiredSourceVersionStringType">
		<simpleContent>
			<extension base="string">
				<attribute name="source" type="string" use="required"/>
				<attribute name="ref" type="string"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="titleStringType">
		<simpleContent>
			<extension base="string">
				<attribute name="titleType"/>
				<attributeGroup ref="sourceVersionGroup"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="subjectStringType">
		<simpleContent>
			<extension base="string">
				<attribute name="subjectType" type="string"/>
				<attributeGroup ref="sourceVersionGroup"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="descriptionStringType">
		<simpleContent>
			<extension base="string">
				<attributeGroup ref="descriptionTypeSourceVersionGroup"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="affiliatedStringType">
		<simpleContent>
			<extension base="string">
				<attribute name="affiliation"/>
				<attribute name="ref"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="contributorStringType">
		<simpleContent>
			<extension base="string">
				<attributeGroup ref="sourceVersionGroup"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="technicalStringType">
		<simpleContent>
			<extension base="string">
				<attribute name="unitsOfMeasure"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="annotationStringType">
		<simpleContent>
			<extension base="string">
				<attribute name="annotationType"/>
			</extension>
		</simpleContent>
	</complexType>
	<complexType name="rightsSummaryType">
		<choice>
			<element maxOccurs="1" minOccurs="0" name="rightsSummary" type="sourceVersionStringType">
			</element>
			<element maxOccurs="1" minOccurs="0" name="rightsLink" type="rightsLinkType">
			</element>
		</choice>
	</complexType>
	<complexType name="rightsLinkType">
		<simpleContent>
			<extension base="anyURI">
				<attribute name="annotation" type="string"/>
			</extension>
		</simpleContent>
	</complexType>
	<simpleType name="threeLetterCode">
		<restriction base="string">
			<pattern value="([a-z]{3}((;[a-z]{3})?)*)?"/>
		</restriction>
	</simpleType>
	<attributeGroup name="descriptionTypeSourceVersionGroup">
		<attribute name="descriptionType" type="string"/>
	</attributeGroup>
	<attributeGroup name="sourceVersionGroup">
		<attribute name="source" type="string" use="optional"/>
		<attribute name="ref" type="string"/>
	</attributeGroup>
</schema>